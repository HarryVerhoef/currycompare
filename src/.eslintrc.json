{
    "env": {
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended",
        "plugin:prettier/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "jest"
    ],
    "ignorePatterns": [
        "/node_modules/**",
        "/dist/**",
        "/prisma/generated/**"
    ],
    "rules": {
    }
}
