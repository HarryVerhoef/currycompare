{
  "name": "currycompare",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "ci:interpolate": "./node_modules/.bin/ts-node ./ci/scripts/interpolate.ts",
    "db:generate": "npx prisma generate",
    "db:migrate:deploy": "npx prisma migrate deploy",
    "db:migrate:local": "./node_modules/.bin/dotenv -e .env.local -- npx prisma migrate dev",
    "db:reset:local": "./node_modules/.bin/dotenv -e .env.local -- npx prisma migrate reset --force",
    "deploy:lambdas": "./node_modules/.bin/ts-node ./ci/scripts/triggerLambdaDeployments.ts",
    "lint:check": "./node_modules/.bin/eslint . --ext .ts",
    "lint:fix": "./node_modules/.bin/eslint . --ext .ts --fix",
    "log:db:url:dev": "./node_modules/.bin/dotenv -e .env.dev -- npx ts-node ./utils/buildDatabaseURL/logDatabaseURL.ts",
    "serve:api:local": "npx ts-node ./index.ts",
    "serve:local": "docker compose --env-file .env.container up",
    "serve:local:background": "docker compose --env-file .env.container up -d",
    "serve:local:clean": "docker compose --env-file .env.container up --build",
    "serve:local:clean:background": "docker compose --env-file .env.container up --build -d",
    "test:integration": "CURRYCOMPARE_ENVIRONMENT=local ./node_modules/.bin/jest --config jest.integration.config.json --detectOpenHandles --silent",
    "test:unit": "./node_modules/.bin/jest --config jest.unit.config.json",
    "transpile": "./node_modules/.bin/ts-node ./ci/scripts/transpile.ts"
  },
  "prisma": {
    "seed": "./node_modules/.bin/ts-node ./prisma/seed.ts"
  },
  "author": "Harry Verhoef",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/client-secrets-manager": "^3.592.0",
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-typescript": "^7.23.3",
    "@types/aws-lambda": "^8.10.137",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "aws-sdk-client-mock": "^4.0.1",
    "babel-jest": "^29.7.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "esbuild": "^0.21.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "express": "^4.19.2",
    "jest": "^29.7.0",
    "jest-mock-extended": "^3.0.7",
    "octokit": "^3.2.1",
    "prettier": "^3.2.5",
    "prisma": "^5.9.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@prisma/client": "^5.9.1",
    "axios": "^1.7.2",
    "fp-ts": "^2.16.6",
    "io-ts": "^2.2.21"
  }
